<odoo>
    <data>
        <record model="ir.ui.view" id="kompetensi_inti_list_view">
            <field name="name">kompetensi.inti.list.view</field>
            <field name="model">kompetensi.inti</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="desc"/>
                </tree>
            </field>
        </record>


        <record model="ir.actions.act_window" id="kompetensi_inti_action_window">
            <field name="name">Kompetensi Inti</field>
            <field name="res_model">kompetensi.inti</field>
            <field name="view_mode">tree,form</field>
        </record>

        <record id="kompetensi_inti_form_view" model="ir.ui.view">
            <field name="name">kompetensi.inti.form</field>
            <field name="model">kompetensi.inti</field>
            <field name="arch" type="xml">
                <form duplicate="0" string="Kompetensi Inti" class="o_sale_order">
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="desc"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>


    </data>
</odoo>