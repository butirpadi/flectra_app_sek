<odoo>
    <data noupdate="1" >
        <record model="ir.module.category" id="kurikulum_module_management">
            <field name="name">Kurikulum</field>
            <field name="description">User access level for this module</field>
            <field name="sequence">9</field>
        </record>

        <record id="kurikulum_guru" model="res.groups">
            <field name="name">Guru/Wali Kelas</field>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
            <field name="users" eval="[(4, ref('base.user_root'))]"/>
            <field name="category_id" ref="siswa_kurikulum.kurikulum_module_management"/>
        </record>

        <record id="kurikulum_manager" model="res.groups">
            <field name="name">Manager</field>
            <field name="implied_ids" eval="[(4, ref('siswa_kurikulum.kurikulum_guru'))]"/>
            <field name="users" eval="[(4, ref('base.user_root'))]"/>
            <field name="category_id" ref="siswa_kurikulum.kurikulum_module_management"/>
        </record>

        <record id="kurikulum_administrator" model="res.groups">
            <field name="name">Administrator</field>
            <field name="implied_ids" eval="[(4, ref('siswa_kurikulum.kurikulum_manager'))]"/>
            <field name="users" eval="[(4, ref('base.user_root'))]"/>
            <field name="category_id" ref="siswa_kurikulum.kurikulum_module_management"/>
        </record>
    </data>
</odoo>


