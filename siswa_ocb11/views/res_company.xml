<odoo>
	<data>

		<record id="siswa_ocb11_view_company_tree" model="ir.ui.view">
			<field name="name">siswa.ocb11.view.company.tree</field>
			<field name="model">res.company</field>
			<field name="inherit_id" ref="base.view_company_tree" />
			<field name="priority" eval="20" />
			<field name="arch" type="xml">
				<xpath expr='//tree' position="attributes">
					<attribute name="create">0</attribute>
				</xpath>
			</field>
		</record>

		<record id="siswa_ocb11_view_company_form" model="ir.ui.view">
			<field name="name">siswa.ocb11.view.company.form</field>
			<field name="model">res.company</field>
			<field name="inherit_id" ref="base.view_company_form" />
			<field name="priority" eval="22" />
			<field name="arch" type="xml">
				<xpath expr='//form' position="attributes">
					<attribute name="create">0</attribute>
					<attribute name="delete">0</attribute>
				</xpath>
			</field>
		</record>

	</data>
</odoo>