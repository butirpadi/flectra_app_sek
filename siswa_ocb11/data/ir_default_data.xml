<odoo>
	<data noupdate="1" >
		<function id="function_set_domain_res_user"
			model="res.partner" name="set_default_domain_user" />

		<!-- Set Rupiah Active Only -->
		<function id="function_set_active_rupiah"
			model="res.currency" name="set_active_rupiah_only" />

		<!-- set default logo modoo -->
		<function id="function_set_modoo_logo" model="res.company"
			name="set_modoo_logo" />

		<!-- Set date format to indonesian format -->
		<function id="function_update_date_format_to_id_format"
			model="res.lang" name="update_date_format_to_id_format" />

		<record model="siswa.setting"
			id="siswa_number_default_setting">
			<field name="default_siswa_number">induk</field>
		</record>

		<record id="login_form_disable_footer" model="ir.config_parameter">
            <field name="key">login_form_disable_footer</field>
            <field name="value">False</field>
        </record>

		<record id="login_form_disable_database_manager" model="ir.config_parameter">
            <field name="key">login_form_disable_database_manager</field>
            <field name="value">False</field>
        </record>

	</data>
</odoo>