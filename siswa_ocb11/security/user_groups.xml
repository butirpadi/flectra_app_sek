<odoo>
    <data>
        <record model="ir.module.category" id="module_category_siswa_base_management">
            <field name="name">Siswa Application</field>
            <field name="description">User access level for this module</field>
            <field name="sequence">3</field>
        </record>

        <record id="group_siswa_base_user" model="res.groups">
            <field name="name">User</field>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
            <field name="category_id" ref="module_category_siswa_base_management"/>
        </record>

        <record id="group_siswa_base_admin" model="res.groups">
            <field name="name">Administrator</field>
            <field name="implied_ids" eval="[(4, ref('group_siswa_base_user'))]"/>
            <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
            <field name="category_id" ref="module_category_siswa_base_management"/>
        </record>
    </data>
</odoo>


